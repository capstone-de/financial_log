<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <View
            android:id="@+id/view6"
            android:layout_width="match_parent"
            android:layout_height="175dp"
            android:layout_marginTop="38dp"
            android:background="@drawable/diarylist_back"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp" />

    <TextView
            android:id="@+id/feed_text"
            android:layout_width="330dp"
            android:layout_height="80dp"
            android:fontFamily="@font/gmarketsansttfmedium"
            android:text="타인의 일기 내용 표시"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="@+id/view6"
            android:layout_marginTop="48dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="31dp"
            android:layout_marginEnd="32dp"
            app:layout_constraintEnd_toEndOf="parent" />

    <TextView
            android:id="@+id/nickname_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="52dp"
            android:fontFamily="@font/gmarketsansttfmedium"
            android:text="나는 개똥벌레"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

    <TextView
            android:id="@+id/location_tv"
            android:layout_width="70dp"
            android:layout_height="20dp"
            android:fontFamily="@font/gmarketsansttfmedium"
            android:text="광진구"
            tools:ignore="MissingConstraints"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="46dp" />

    <ImageView
            android:id="@+id/location_im"
            android:layout_width="20dp"
            android:layout_height="24dp"
            tools:ignore="MissingConstraints"
            app:srcCompat="@drawable/ei_location"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/location_tv"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="43dp"
            app:layout_constraintHorizontal_bias="1.0" />

    <View
            android:id="@+id/view3"
            android:layout_width="150dp"
            android:layout_height="1dp"
            android:layout_marginTop="72dp"
            android:accessibilityHeading="false"
            android:background="#989797"
            android:backgroundTint="#989797"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.111"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

    <TextView
            android:id="@+id/tag_dr"
            android:layout_width="300dp"
            android:layout_height="30dp"
            android:layout_marginTop="178dp"
            android:layout_marginEnd="50dp"
            android:fontFamily="@font/gmarketsansttfmedium"
            android:text="# 건대입구역"
            android:textColor="#624E19"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

    <TextView
            android:id="@+id/day_tv"
            android:layout_width="130dp"
            android:layout_height="17dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="115dp"
            android:fontFamily="@font/gmarketsansttfmedium"
            android:text="2024"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

    <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginBottom="218dp"
            android:background="@color/gray"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>