<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="InvalidId">

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:padding="0dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/left_check"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="6dp" />

    <Button
        android:id="@+id/save_expend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="저장"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="651dp" />

    <com.google.android.material.chip.ChipGroup
        android:layout_width="390dp"
        android:layout_height="123dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="274dp">

        <com.google.android.material.chip.Chip
            android:id="@+id/tax_ex"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="세금" />

        <com.google.android.material.chip.Chip
            android:id="@+id/food_ex"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="식비" />

        <com.google.android.material.chip.Chip
            android:id="@+id/living"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="주거/통신" />

        <com.google.android.material.chip.Chip
            android:id="@+id/traffic"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="교통/차량" />

        <com.google.android.material.chip.Chip
            android:id="@+id/education"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="교육" />

        <com.google.android.material.chip.Chip
            android:id="@+id/dues"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="경조사/회비" />

        <com.google.android.material.chip.Chip
            android:id="@+id/cultual"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="문화/생활" />


        <com.google.android.material.chip.Chip
            android:id="@+id/medical_ex"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="병원/약국" />

        <com.google.android.material.chip.Chip
            android:id="@+id/shopping"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:text="쇼핑" />

        <com.google.android.material.chip.Chip
            android:id="@+id/etc"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:text="기타" />

    </com.google.android.material.chip.ChipGroup>

    <Button
        android:id="@+id/button2"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:text="지출"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="73dp" />

    <EditText
        android:id="@+id/category_ed"
        android:layout_width="275dp"
        android:layout_height="48dp"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="209dp" />

    <TextView
        android:id="@+id/category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="카테고리"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/ex_satis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="소비만족도"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="557dp" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="177dp"
        android:layout_height="30dp"
        android:max="100"
        android:progress="10"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="548dp" />

    <TextView
        android:id="@+id/totalTv"
        style="@style/TextAppearance.Material3.LabelLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/seekBar"
        android:layout_alignParentEnd="true"
        android:text="100"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="284dp"
        tools:layout_editor_absoluteY="578dp" />

    <TextView
        android:id="@+id/progressTv"
        style="@style/TextAppearance.Material3.LabelLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/seekBar"
        android:text="0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="578dp" />

    <TextView
        android:id="@+id/together"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="함께한 사람"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="502dp" />

    <EditText
        android:id="@+id/money_ed"
        android:layout_width="272dp"
        android:layout_height="48dp"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="150dp" />

    <EditText
        android:id="@+id/together_ed"
        android:layout_width="101dp"
        android:layout_height="48dp"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="485dp" />

    <TextView
        android:id="@+id/money"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:text="금액"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="150dp" />

    <TextView
        android:id="@+id/name_shop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="상호명"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="455dp" />


    <EditText
        android:id="@+id/name_shop_ed"
        android:layout_width="275dp"
        android:layout_height="48dp"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="435dp" />


    <Button
        android:id="@+id/button"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:text="수입"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="73dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="혼자"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="485dp" />


</androidx.constraintlayout.widget.ConstraintLayout>