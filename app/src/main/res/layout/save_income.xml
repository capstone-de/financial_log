<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:padding="0dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/left_check"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="6dp" />

    <EditText
        android:id="@+id/money_ed"
        android:layout_width="272dp"
        android:layout_height="49dp"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="150dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:text="수입"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="93dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:text="지출"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/memo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="메모"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="357dp" />

    <EditText
        android:id="@+id/memo_ed"
        android:layout_width="275dp"
        android:layout_height="48dp"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="357dp" />

    <com.google.android.material.chip.ChipGroup
        android:layout_width="307dp"
        android:layout_height="58dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="269dp">

        <com.google.android.material.chip.Chip
            android:id="@+id/pay_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="월급" />

        <com.google.android.material.chip.Chip
            android:id="@+id/extra_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="부수입" />

        <com.google.android.material.chip.Chip
            android:id="@+id/financial_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="금융수익" />

        <com.google.android.material.chip.Chip
            android:id="@+id/etc_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="기타" />


    </com.google.android.material.chip.ChipGroup>

    <EditText
        android:id="@+id/category_ed"
        android:layout_width="275dp"
        android:layout_height="48dp"
        tools:ignore="SpeakableTextPresentCheck,MissingConstraints"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="209dp" />

    <TextView
        android:id="@+id/money"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:text="금액"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="150dp" />


    <TextView
        android:id="@+id/category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="카테고리"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="218dp" />

    <Button
        android:id="@+id/save_expend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="저장"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="495dp" />


</androidx.constraintlayout.widget.ConstraintLayout>